.section
	.row
		h5.center-align USERS
			
	- if @org_users.count < 1
		.row
			p.col.s11.center-align No user exists!
	- else
		ul.collection
			- @org_users.each do |org_user|
				li.row.collection-item
					.col.s4
						p
							| #{org_user[:user][:name]}
							br/
							| #{org_user[:user][:email]}
					.col.s4
						p
							| #{org_user[:user][:gender]}
							br/
							| #{org_user[:user][:contact_no]}
					.col.s4 #{org_user[:user][:address]}
					